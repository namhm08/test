<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>FilterAuthController</filter-name>
        <filter-class>controller.filter.FilterAuthController</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FilterAuthController</filter-name>
        <url-pattern>/auth/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Auth_Test</servlet-name>
        <servlet-class>controller.authentication.Auth_Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Auth_Dosmth</servlet-name>
        <servlet-class>controller.authentication.Auth_Dosmth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>controller.test.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.authentication.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Auth_Test</servlet-name>
        <url-pattern>/auth/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Auth_Dosmth</servlet-name>
        <url-pattern>/auth/dosmth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>
